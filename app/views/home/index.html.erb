<table class="table">
  <% MultipleChoiceQuestion.all.each do |question| %>
      <tr>
        <td><%= link_to question.question, question_path(question) %></td>
        <td>
            <% question.answers.each do |key, value| %>
                 <%= "#{key} #{value}"%>
            <% end %>
        </td>
        <td>
          <%= link_to "Delete", question_path(question), method: "DELETE", class: "btn" %>
        </td>
      </tr>
  <% end %>
</table>
<%= link_to 'New Question', new_question_path %>
